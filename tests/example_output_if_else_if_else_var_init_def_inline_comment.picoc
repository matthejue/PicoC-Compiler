// in:10
// expected:1

void main() {
  // complex initialisation
  int var = input() + 1;
  // variable definition
  char var2;
  // if, else if, else
  if (var > 100) {
    var2 = 0;
  } else if (/* inline comment */ var > 10) {
    var2 = 1;
  } else if (var) {
    var2 = 2;
  } else
    var2 = 3;
  print(var2);
}
