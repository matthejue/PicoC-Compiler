// from the Introduction to Programming Lecture by Peter Thiemann


int power_iterative(int a, int n, int acc) {
    while (n != 0) {
        n = n - 1;
        acc = acc * a;
    }
    return acc;
}


void main() {
  print(power_iterative(input(), input(), 1));
}
