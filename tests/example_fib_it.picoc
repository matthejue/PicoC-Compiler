// in:10
// expected:55
// datasegment:32

int fib(int fib_nr) {
  int i = 2;
  int fib_i;
  int fib_i_1 = 1;
  int fib_i_2 = 1;
  while (i < fib_nr) {
    fib_i = fib_i_1 + fib_i_2;
    fib_i_2 = fib_i_1;
    fib_i_1 = fib_i;
    i = i + 1;
  }
  return fib_i;
}

void main() {
  print(fib(input()));
}
